{
  "expo": {
    "name": "recetasfr",
    "slug": "recetasfr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-green.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-green.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.recetasfr.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.recetasfr.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
              "scheme": "com.recetasfr.app",
              "host": "oauthredirect"
            },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-app.png",
        "backgroundColor": "#0c5b25"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/logo-light.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "owner": "moyas249",
    "extra": {
      "eas": {
        "projectId": "b9180e55-b091-45ce-987e-139ddda5fa9d"
      }
    }
  }
}
